<li class="defin">
  <div class="word"><%= defin.escape('word') %></div>
  <br>
  <div class="def"><%= defin.escape('defin') %> </div>
  <br>
  <div class="example"> <%= defin.escape('ex_sentence') %></div> <br>
  <strong>
  by <a
    href="<%='#/users/' + defin.get('author_id')%>">
    <%= defin.author().get('user_name') %>
    </a>
    <%= new Date(defin.get('created_at')).toDateString() %>
</strong>
<br>
<button class="upvote" data-id="<%=defin.get('id') %>">
  <strong>Yaaaas <%= defin.votes().get('up_count') %> </strong>
</button>
<button class="downvote" data-id="<%=defin.get('id') %>">
  <strong>Nah, chill   <%= defin.votes().get('down_count') %> </strong>
</button>



</li>

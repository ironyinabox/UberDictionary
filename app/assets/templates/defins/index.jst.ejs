<h1>Listing All Defins</h1>
<ul class="defin-list">
<% defins.forEach(function(defin) { %>
  <li class="defin">
    <div class="word"><%= defin.escape('word') %></div>
    <br>
    <div class="def"><%= defin.escape('defin') %> </div>
    <br>
    <div class="example"> <%= defin.escape('ex_sentence') %></div> <br>
    <strong>
    by <a href="<%='#/users/' + defin.get('author_id')%>"><%= defin.author().get('user_name') %></a>
  </strong> <br>
    <button class="upvote">Thumbs Up</button> <button class="downvote">Thumbs Down</button>
  </li>
<% }) %>
</ul>

<a href="#/defins/new"> New Def</a>
